{
	"name": "my-react-template",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"autoprefixer": "^10.1.0",
		"axios": "^0.21.0",
		"node-sass": "^5.0.0",
		"postcss": "^8.2.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-scripts": "4.0.1",
		"tailwindcss": "^2.0.2",
		"web-vitals": "^0.2.4"
	},
	"scripts": {
		"twdBuild": "tailwind build src/stylesheets/css/twd-input.css -o src/stylesheets/css/twd-output.css",
		"sass": "node-sass --watch src/stylesheets/sass -o src/stylesheets/css",
		"start": "yarn twdBuild && react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
